{
  "manifest_version": 2,
  "name": "Pixiv Bookmark with Star",
  "version": "1.2",
  "description": "pixivでブックマークするときに同時に10点評価する",
  "icons": 
  {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "content_scripts": [
    {
      "js": [
        "pixiv_bookmark_with_star.user.js"
      ],
      "matches": [
        "http://www.pixiv.net/member_illust.php?mode=medium&illust_id=*",
        "http://www.pixiv.net/novel/show.php?id=*"
      ]
    }
  ]
}
